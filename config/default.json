			"jamulus/+/state/newconnection",
			"jamulus/+/state/recording/restart",
			"jamulus/+/state/recording/start",
			"jamulus/+/state/recording/stop",
			"jamulus/+/state/recording/endthread",
			"jamulus/+/state/response/configuration/get",
			"jamulus/+/state/response/connections/get",
			"jamulus/+/state/serverfull"
